<%= simple_form_for(@programme) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.association :user, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :start_date_time, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :end_date_time, input_html: { class: 'form-control'} %>
  </div>

  <div class="form-group">
    <%= f.button :submit %>
  </div>
<% end %>
