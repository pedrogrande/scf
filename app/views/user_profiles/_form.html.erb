<%= simple_form_for(@user_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :name, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :phone, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :tagline, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :about, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :image, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :facebook, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :linkedin, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :googlegithub, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :twitter, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :blog, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.input :show_email, input_html: { class: 'form-control'} %>
  </div>
  <div class="form-group">
    <%= f.association :user, input_html: { class: 'form-control'} %>
  </div>

  <div class="form-group">
    <%= f.button :submit %>
  </div>
<% end %>
